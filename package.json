{
	"name": "ai-code",
	"version": "0.1.0",
	"description": "A comprehensive agent orchestration framework for managing directory-based agent responsibilities and inter-agent communication",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"keywords": ["ai", "agent", "orchestration", "automation", "typescript", "code", "workflow"],
	"repository": {
		"type": "git",
		"url": "https://github.com/mgorunuch/ai-code.git"
	},
	"bugs": {
		"url": "https://github.com/mgorunuch/ai-code/issues"
	},
	"homepage": "https://github.com/mgorunuch/ai-code#readme",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"ink": "^4.1.0",
		"meow": "^11.0.0",
		"minimatch": "^10.0.3",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
